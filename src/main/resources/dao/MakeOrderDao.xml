<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bo.ucb.edu.ingsoft.dao.MakeOrderDao">
    <insert id="create" parameterType="bo.ucb.edu.ingsoft.dto.MakeOrder">
        INSERT INTO order
        VALUES (
            null, #{productId}, #{userId}, #{deliveryId}, #{addressId},
            #{date},#{status},#{total}, 1, #{txHost}, #{txUserId}, #{txDate}
        )
    </insert>
</mapper>
