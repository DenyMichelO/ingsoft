<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bo.ucb.edu.ingsoft.dao.HProductDao">
    <insert id="createHProduct" parameterType="bo.ucb.edu.ingsoft.model.HProduct">
        INSERT INTO h_product
        VALUES (
            null, #{productId}, #{brand}, #{model}, #{price}, #{description},
            #{stock}, 1, #{txHost}, #{txUserId}, #{txDate}
        )
    </insert>
</mapper>
