<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bo.ucb.edu.ingsoft.dao.UsertDao">
    <select id="findByContactId" resultType="bo.ucb.edu.ingsoft.dto.User">
        SELECT
               user_id as userId,
               firstname as firstName,
               lastname as firstSurname
        FROM user
        WHERE
            user_id = 2
    </select>
    <insert id="create" parameterType="bo.ucb.edu.ingsoft.dto.User">
        INSERT INTO user VALUES (null, #{addressId},#{firstname}, #{lastname}, #{phone}, #{email},
        #{TxStatus}, #{TxDate}, #{TxId}, #{TxHost}, #{TxUserId})
    </insert>
</mapper>
